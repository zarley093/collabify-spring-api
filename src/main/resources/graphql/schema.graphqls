type Query {
  user(id: ID!): User
  users: [User!]!
}

type User {
  id: ID!
  username: String!
  email: String!
}

input UserInput {
  username: String!
  email: String!
}


type Mutation { 
  createUser(input: UserInput!): User!
  updateUser(id: ID!, input: UserInput): User!
  deleteUser(id: ID!): Boolean!
}